{
	"for_description_of_this_file_see": "https://github.com/function61/turbobob",
	"version_major": 1,
	"project_name": "ruuvinator",
	"builders": [
		{
			"name": "default",
			"dockerfile_path": "build-default.Dockerfile",
			"mount_destination": "/go/src/github.com/function61/ruuvinator",
			"dev_command": [
				"bash"
			],
			"pass_envs": ["BINTRAY_CREDS"],
			"contextless_build": true
		}
	],
	"docker_images": [
		{
			"image": "fn61/ruuvinator",
			"dockerfile_path": "Dockerfile",
			"auth_type": "creds_from_env"
		}
	]
}
